{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        },
        "jsx": "react-jsx",
        "module": "esnext",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "allowJs": true,
        "skipLibCheck": true,
        "strict": false
    },
    "include": [
        "frontend/src/**/*",
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}